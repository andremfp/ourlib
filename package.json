{
  "name": "ourlib",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint 'src/**/*.{js,ts,vue}'",
    "test": "yarn test:firestore",
    "test:firestore": "FIREBASE_DEBUG_MODE=true FUNCTIONS_EMULATOR=true cd functions && yarn build && cd .. && firebase emulators:exec --project=demo-test --inspect-functions --only firestore,functions \"./node_modules/.bin/jest --runInBand  --detectOpenHandles --forceExit --coverage /src/__tests__/firestoreRules.test.ts \"",
    "test:unit": "vitest run",
    "emulator:start": "firebase emulators:start --project=demo-test",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:firestore:rules": "firebase deploy --only firestore:rules",
    "deploy:firestore:indexes": "firebase deploy --only firestore:indexes"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "@ionic/vue": "^8.6.4",
    "@ionic/vue-router": "^8.6.4",
    "@types/jsdom": "^21.1.7",
    "@vueuse/gesture": "^2.0.0",
    "@vueuse/motion": "^2.2.6",
    "@zxing/library": "^0.21.3",
    "dotenv": "^16.4.7",
    "firebase": "^11.2.0",
    "firebase-admin": "^13.0.2",
    "firebase-functions": "^6.3.1",
    "firebase-tools": "^13.29.1",
    "global": "^4.4.0",
    "html5-qrcode": "^2.3.8",
    "ionicons": "^8.0.13",
    "pinia": "^3.0.1",
    "pulltorefreshjs": "^0.1.22",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.21.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@firebase/rules-unit-testing": "^4.0.1",
    "@testing-library/vue": "^8.1.0",
    "@types/firebase": "^3.2.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.2",
    "@types/pulltorefreshjs": "^0.1.7",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vercel/node": "^5.0.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "^9.32.0",
    "fs": "^0.0.1-security",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.3",
    "vitest": "^3.0.6",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.10",
    "workbox-build": "^7.3.0",
    "workbox-window": "^7.3.0"
  }
}
