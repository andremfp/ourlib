{
  "rewrites": [
    {
      "source": "/goodreads-proxy/:path*",
      "destination": "/api/goodreads-proxy/:path*"
    },
    {
      "source": "/goodreads-cover-proxy/:path*",
      "destination": "https://images-na.ssl-images-amazon.com/:path*"
    },
    {
      "source": "/google-proxy/:path*",
      "destination": "https://www.googleapis.com/:path*"
    },
    {
      "source": "/google-cover-proxy/:path*",
      "destination": "http://books.google.com/:path*"
    },
    {
      "source": "/hardcover-proxy/:path*",
      "destination": "https://api.hardcover.app/:path*"
    },
    {
      "source": "/hardcover-cover-proxy/:path*",
      "destination": "https://assets.hardcover.app/:path*"
    }
  ]
}
